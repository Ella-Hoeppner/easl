@{group 0 binding 0}
(var [uniform] dimensions: vec4f)

@{group 0 binding 1}
(var [uniform] time: f32)

(struct VertexInput
  @{location 0} corner-position: vec2f)

(struct VertexOutput
  @{builtin position} vertex-pos: vec4f)

@fragment
(defn fragment [in: VertexOutput]: @{location 0} vec4f
  (let [a (vec2f 0.)
        b: f32 1.
        c]
    (vec4 (vec3f 0.) (.x a))))
