(defn f []
  (let [@var a 0.]
    (for [i 0 (< i 10) (= i (+ i 1))]
      (+= a (f32 (* i i)))
      (break))))

; The following will cause an error, unlike the above, since an expression
; occurs after the break statement
#_(defn f2 []
    (let [@var a 0.]
      (for [i 0 (< i 10) (= i (+ i 1))]
        (break)
        (+= a (f32 (* i i))))))
