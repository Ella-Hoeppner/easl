(defn main []: f32
  (let [@var x 0.]
    (for [i 0i (< i 10) (+= i 1)]
      (+= x 1.))
    (for [i 0u (< i 10) (+= i 1)]
      (+= x 1.))
    (for [i 0. (< i 10.) (+= i 1)]
      (+= x 1.))
    (let [initial-value (vec2f 0.)]
      (for [i
            initial-value
            (< i.x 10.)
            (+= i.y 1)]
        (+= x 1.)))
    x))
